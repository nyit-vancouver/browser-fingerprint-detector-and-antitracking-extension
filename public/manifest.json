{
  "manifest_version": 3,
  "name": "Anti-Tracking",
  "version": "0.0.1",
  "description": "A simple Chrome extension with React.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "./static/js/background.js",
    "scripts": [
      "./static/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["static/js/inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["https://www.baidu.com/*"],
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback",
    "storage",
    "<all_urls>",
    "activeTab",
    "scripting"
  ]
}