{
  "manifest_version": 3,
  "name": "Fringerprint Detector",
  "version": "0.0.1",
  "description": "A simple Chrome extension with React.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./static/js/content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "webRequest",
    "storage",
    "<all_urls>"
  ]
}
